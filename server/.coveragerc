[run]

source =
    api
    mpesa
    ussd

[report]

omit =
    */migrations/*
    */tests/*
    */tests.py
    */test_models.py
    */admin.py
    */apps.py
    */settings.py
    */wsgi.py
    */asgi.py
    */manage.py
    */__init__.py
    */urls.py
    */seed.py
    
exclude_lines =
    pragma: no cover
    def __repr__
    if settings.DEBUG
    if __name__ == '__main__'